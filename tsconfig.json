{
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": ["./agentERC20Base.ts", "./actions", "./typechain"],
  "files": [],
  "ts-node": {
    "transpileOnly": true
  }
}
